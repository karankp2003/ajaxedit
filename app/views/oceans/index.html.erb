<h4>Listing oceans</h4>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @oceans.each do |ocean| %>
  <tr>
    <td><%= ocean.name %></td>
    <td><%= link_to 'Show', ocean %></td>
    <td><%= link_to 'Edit', edit_ocean_path(ocean) %></td>
    <td><%= link_to 'Destroy', ocean, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Ocean', new_ocean_path %>
